<?xml version="1.0" encoding="UTF-8"?>
<!--
 * FacturaScripts table extension for Agentes
 * Adds Rdgarantia-specific fields to the agentes table
 *
 * @author  Solwed
 * @package Rdgarantia Plugin
 -->
<table>
    <column>
        <name>rdg_import_ref</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>rdg_dni</name>
        <type>character varying(20)</type>
    </column>
    <column>
        <name>rdg_last_sync</name>
        <type>timestamp</type>
    </column>
</table>
