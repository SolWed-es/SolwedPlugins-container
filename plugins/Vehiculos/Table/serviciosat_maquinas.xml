<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : serviciosat_maquinas.xml
    Description:
        Estructura de la tabla serviciosat_maquinas.
-->
<table>
    <column>
        <name>codagente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codcliente</name>
        <type>character varying(10)</type>
    </column>
    <column>
        <name>codfabricante</name>
        <type>character varying(8)</type>
    </column>
    <column>
        <name>descripcion</name>
        <type>text</type>
    </column>
    <column>
        <name>fecha</name>
        <type>date</type>
    </column>
    <column>
        <name>idmaquina</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <column>
        <name>nombre</name>
        <type>character varying(100)</type>
        <null>NO</null>
    </column>
    <column>
        <name>numserie</name>
        <type>character varying(100)</type>
    </column>
    <column>
        <name>matricula</name>
        <type>character varying(20)</type>
        <null>NO</null>
    </column>
    <column>
        <name>bastidor</name>
        <type>character varying(17)</type>
        <null>NO</null>
    </column>
    <column>
        <name>kilometros</name>
        <type>integer</type>
    </column>
    <column>
        <name>fecha_matriculacion</name>
        <type>date</type>
    </column>
    <column>
        <name>color</name>
        <type>character varying(50)</type>
    </column>
    <column>
        <name>combustible</name>
        <type>character varying(20)</type>
    </column>
    <constraint>
        <name>serviciosat_maquinas_pkey</name>
        <type>PRIMARY KEY (idmaquina)</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_maquinas_clientes</name>
        <type>FOREIGN KEY (codcliente) REFERENCES clientes (codcliente) ON DELETE RESTRICT ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_serviciosat_maquinas_fabricantes</name>
        <type>FOREIGN KEY (codfabricante) REFERENCES fabricantes (codfabricante) ON DELETE SET NULL ON UPDATE CASCADE</type>
    </constraint>
</table>